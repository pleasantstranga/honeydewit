<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/listsHomeLayout"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical">


	<com.honeydewit.adapters.draganddrop.DragSortListView
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:dslv="http://schemas.android.com/apk/res-auto"
		android:id="@+id/list"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:choiceMode="multipleChoice"
		dslv:drag_enabled="true"
		dslv:collapsed_height="2dp"
		dslv:drag_scroll_start="0.33"
		dslv:max_drag_scroll_speed="0.5"
		dslv:float_alpha="0.6"
		dslv:slide_shuffle_speed="0.3"
		dslv:track_drag_sort="true"
		dslv:use_default_controller="true"
		dslv:drag_handle_id="@id/drag_handle"
		dslv:sort_enabled="true"
		dslv:remove_enabled="false"
		dslv:drag_start_mode="onMove"/>


</RelativeLayout><!-- From: file:/Users/aaronbernstein/Documents/workspace/HoneyDewIt/app/src/main/res/layout/listshome.xml -->