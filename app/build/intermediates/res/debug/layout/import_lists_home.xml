<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/importErrorLayout"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical">
	
	<include
	    android:id="@+id/headerInclude"
	    android:layout_alignParentTop="true" 
		android:layout_width="fill_parent"
   		android:layout_height="wrap_content"
   		layout="@layout/header" />
 	
	<ListView
	    android:layout_below="@+id/headerInclude"
	    android:id="@+id/importList"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent" 
	   />
	<LinearLayout android:id="@+id/buttonsRow" android:layout_alignParentBottom="true" android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="wrap_content">
	     <Button android:layout_gravity="start" android:layout_weight=".5"  style="?android:attr/buttonBarButtonStyle" android:id="@+id/cancelBtn" android:layout_height="wrap_content" android:layout_width="0dp" android:text="@string/cancel"/>
	     <Button android:layout_gravity="end" android:layout_weight=".5"  style="?android:attr/buttonBarButtonStyle" android:id="@+id/importBtn" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/importList"/>
	</LinearLayout>  
</RelativeLayout><!-- From: file:/Users/aaronbernstein/Documents/workspace/HoneyDewIt/app/src/main/res/layout/import_lists_home.xml -->