<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/monthFrag"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent" >

	<com.honeydewit.calendar.customviews.CalendarView xmlns:app="http://schemas.android.com/apk/res/com.honeydewit"
	    android:id="@+id/calendarView"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		app:saveMonthOnConfigChange="true"
	/>
	

	<ListView android:id="@android:id/list" android:layout_width="0dip"
		android:layout_height="0dip" android:visibility="gone"/>
	
</LinearLayout>
